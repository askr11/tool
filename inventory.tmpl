[ubuntu_nodes]
%{ for ip in ubuntu_hosts ~}
${ip}
%{ endfor ~}

[bastion]
%{ for ip in bastion_hosts ~}
${ip}
%{ endfor ~}

[ubuntu_nodes:vars]
ansible_ssh_private_key_file=/var/lib/jenkins/workspace/tool/keyins.pem
ansible_user=ubuntu
ansible_ssh_common_args='-o ProxyJump=ubuntu@{{bastion_hosts}}'
